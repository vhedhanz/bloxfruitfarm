wait(0.5) 

local ba=Instance.new("ScreenGui") 

local ca=Instance.new("TextLabel")  -- Header (Draggable)

local da=Instance.new("Frame")      -- Kotak Konten (Diperbesar)

local _b=Instance.new("TextLabel")  -- Credit

local ab=Instance.new("TextLabel")  -- Status Anti-AFK

local FPSText = Instance.new("TextLabel")  -- Label untuk FPS

local CloseButton = Instance.new("TextButton") -- Tombol Close



-- ====================

-- PROPERTI GUI UTAMA & HEADER

-- ====================



ba.Parent=game.CoreGui 

ba.ZIndexBehavior=Enum.ZIndexBehavior.Sibling 



-- Elemen Header (ca) 

ca.Parent=ba 

ca.Active=true 

ca.BackgroundColor3=Color3.new(0, 0, 0) 

ca.Draggable=true 

ca.Position=UDim2.new(0.698610067,0,0.098096624,0) 

ca.Size=UDim2.new(0,200,0,40) 

ca.Font=Enum.Font.SourceSansSemibold 

ca.Text="Anti AFK" 

ca.TextColor3=Color3.new(0,1,1) 

ca.TextSize=22 



-- Tombol Close

CloseButton.Parent = ca

CloseButton.BackgroundColor3 = Color3.new(1, 0, 0) 

CloseButton.Size = UDim2.new(0, 20, 0, 20)

CloseButton.Position = UDim2.new(1, -25, 0.5, -10)

CloseButton.Font = Enum.Font.SourceSansBold

CloseButton.Text = "X"

CloseButton.TextColor3 = Color3.new(1, 1, 1)

CloseButton.TextSize = 18



-- Elemen Frame (da) - Kotak Konten

da.Parent=ca 

da.BackgroundColor3=Color3.new(0, 0, 0) 

da.Position=UDim2.new(0,0,1.0192306,0) 

da.Size=UDim2.new(0,200,0,120) 



-- ====================

-- ELEMEN KONTEN (STATUS, FPS, CREDIT)

-- ====================



-- Elemen TextLabel (ab) - "Status : Active" 

ab.Parent=da 

ab.BackgroundColor3=Color3.new(0, 0, 0) 

ab.Position=UDim2.new(0,0,0.05,0) 

ab.Size=UDim2.new(0,200,0,40) 

ab.Font=Enum.Font.ArialBold 

ab.Text="Status : Active" 

ab.TextColor3=Color3.new(0,1,1) 

ab.TextSize=20 



-- Elemen TextLabel (FPSText) - FPS Status

FPSText.Parent = da

FPSText.BackgroundColor3 = Color3.new(0, 0, 0) 

FPSText.Position = UDim2.new(0, 0, 0.4, 0) -- DI SINI

FPSText.Size = UDim2.new(0, 200, 0, 25)

FPSText.Font = Enum.Font.SourceSans

FPSText.Text = "FPS: 60" -- Teks ini akan diperbarui oleh logika di bawah

FPSText.TextColor3 = Color3.new(1, 1, 1)

FPSText.TextSize = 16

FPSText.TextXAlignment = Enum.TextXAlignment.Center



-- Elemen TextLabel (_b) - "Made by VhedhanZ" 

_b.Parent=da 

_b.BackgroundColor3=Color3.new(0, 0, 0) 

_b.Position=UDim2.new(0,0,0.75,0) 

_b.Size=UDim2.new(0,200,0,21) 

_b.Font=Enum.Font.SourceSans

_b.Text="Made by VhedhanZ" 

_b.TextColor3=Color3.new(1, 0, 0)

_b.TextSize=16



-- ====================

-- LOGIKA FUNGSI

-- ====================



local bb=game:service'VirtualUser' 

local Players = game:service'Players'

local LocalPlayer = Players.LocalPlayer

local RunService = game:service'RunService' -- Diperlukan



-- LOGIKA ANTI-AFK 

LocalPlayer.Idled:connect(function() 

    bb:CaptureController() 

    bb:ClickButton2(Vector2.new()) 

    ab.Text="jangan usir aku roblox !" 

    wait(2) 

    ab.Text="Status : Active" 

end)



-- LOGIKA FPS (Ini adalah fungsi FPS yang diminta)

RunService.Heartbeat:connect(function(step)

    -- Menghitung FPS (1 dibagi waktu antar frame)

    local fps = math.floor(1 / step)

    FPSText.Text = "FPS: " .. fps -- Memperbarui label

end)



-- LOGIKA TOMBOL CLOSE

CloseButton.MouseButton1Click:connect(function()

    ba:Destroy() 

end)
