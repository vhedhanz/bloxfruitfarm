ca.TextColor3=Color3.new(0,1,1)
ca.TextSize=22

-- Elemen Frame (da) - Kotak Konten
da.Parent=ca
da.BackgroundColor3=Color3.new(0.196078,0.196078,0.196078)
da.BackgroundTransparency = 0.5 -- Contoh transparansi untuk frame konten (lebih transparan)
da.Position=UDim2.new(0,0,1.0192306,0)
da.Size=UDim2.new(0,200,0,91)

-- Elemen TextLabel (_b) - "Made by VhedhanZ"
_b.Parent=da
